theory BinaryLaws_Images
  imports Main
begin


text ‹--- Distributive (Phân phối) ---›
lemma distributive_and_or1:
  "(a ∧ (b ∨ c)) = ((a ∧ b) ∨ (a ∧ c))"
  by auto

lemma distributive_and_or2:
  "(a ∨ (b ∧ c)) = ((a ∨ b) ∧ (a ∨ c))"
  by auto

lemma distributive_and_or3:
  "(a ∧ b ∨ a ∧ c) = (a ∧ (b ∨ c))"
  by auto

lemma distributive_and_or4:
  "(((b ∨ a) ∧ (c ∨ a))) = (a ∨ b ∧ c)"
  by auto

end